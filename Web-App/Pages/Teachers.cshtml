@page
@model Web_App.Pages.TeachersModel
@{
    ViewData["Title"] = "Teachers";
}

<head>
    <link rel="icon" type="image/png" sizes="32x32" href="~/icon2.png">
</head>

<body>
    <div class="container">
        @foreach (var room in Model.Teachers)
        {
            <div class="card">
                <div class="card-text">@room.fullName</div>
            </div>
        }
    </div>
</body>